apiVersion: 1

editable: true

datasources:
  - name: InfluxDB
    type: inflxudb
    access: proxy
    url: {{ influxdb_url }}
    secureJsonData:
      token: {{ influxdb_init_admin_token }}
    jsonData:
      version: Flux
      httpMode: "POST"
      organization: {{ influxdb_init_org }}
      defaultBucket: {{ influxdb_init_bucket }}
      tlsSkipVerify: true